{
  "name": "vite-plugin-react-path-router",
  "version": "1.0.0",
  "private": false,
  "files": [
    "./client",
    "./dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean-artifacts": "rimraf ../tsconfig.client.tsbuildinfo tsconfig.tsbuildinfo",
    "build:client": "npm run clean-artifacts rimraf client && tsc --project tsconfig.client.json",
    "build:lib": "npm run clean-artifacts rimraf dist ../tsconfig.client.tsbuildinfo tsconfig.tsbuildinfo && tsc --project tsconfig.json",
    "build": "run-p build:*",
    "watch:client": "npm run clean-artifacts rimraf client && tsc --project tsconfig.client.json --watch",
    "watch:lib": "npm run clean-artifacts rimraf dist && tsc --project tsconfig.json --watch",
    "watch": "run-p watch:*"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./client": {
      "import": {
        "types": "./client/index.d.ts",
        "default": "./client/index.js"
      }
    },
    "./plugin": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "vite": "^2.9.9"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.29",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.0",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "bundlesize": "^0.18.1",
    "debug": "^4.3.4",
    "jest": "^28.0.2",
    "jest-junit": "^13.2.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "path-to-regexp": "^6.2.1"
  }
}
